import prelude version 1.0.0 as prelude
import std version 1.0.0 as std
import cvm_base version 1.0.0 as cvm_base
import cvm_executable version 1.0.0 as cvm_executable
import cvm_clientside version 1.0.0 as cvm_clientside
import unqualified globals version 1.0.0


@executable
def do_executable() -> str:
    return "foo"

@executable
def do_executable_error() -> None:
    cvm.error("This is an executable error")


@clientside
def run_executable() -> None:

    with PostTxArgs(PUBLIC, []):
        do_executable()


@clientside
def run_clientside() -> str:
    return "bar"


@clientside
def run_clientside_error() -> None:
    cvm.error("This is a clientside error")


@clientside
def run_executable_error() -> None:
    with PostTxArgs(PUBLIC, []):
        do_executable_error()
