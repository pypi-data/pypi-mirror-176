{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "port": {
      "type": ["integer", "string"],
      "is_port": true,
      "default": 8080
    },
    "username": { "type": "string" },
    "password": { "type": "string" }
  },
  "additionalProperties": true,
  "required": [
    "port",
    "username",
    "password"
  ]
}
